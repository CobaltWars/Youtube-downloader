{
  "version": 2,
  "builds": [
    {
      "src": "api/download.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "ressource/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/download",
      "dest": "/api/download.js",
      "methods": ["POST"]
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/ressource/(.*)",
      "dest": "/ressource/$1"
    }
  ]
}

